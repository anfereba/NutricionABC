<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentosCliente.OpcionUnoCliente">

    <RatingBar
        android:id="@+id/RatingCalifNutri"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="192dp"
        android:background="#2962FF"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:rating="2" />

    <Button
        android:id="@+id/BtnCalifNutri"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:background="@color/purple_500"
        android:text="Calificar"
        android:textColor="#00BFA5"
        android:textSize="20sp"
        android:textStyle="bold"
        android:translationX="50dp"
       />

    <TextView
        android:id="@+id/TextCalifNutri"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="61dp"
        android:background="#75DACAC5"
        android:text="Calificacion Nutriologo"
        android:textColor="#0C69BA"
        android:textSize="24sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/EditTextCalifNutri"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="119dp"
        android:ems="10"
        android:hint="ID Nutriologo"
        android:inputType="textPersonName" />

</FrameLayout>